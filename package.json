{
  "name": "eth-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
    "prd": "NODE_ENV=production pm2 start bin/www",
    "wallet": "task=wallet node ./server/common",
    "transaction": "task=getTransaction node ./server/common",
    "total": "task=getTotal node ./server/common",
    "dtc": "task=deployTokenContract node ./server/common",
    "dev-geth": "bash geth.dev.sh"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "debug": "^2.6.3",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "node-schedule": "^1.2.5",
    "request": "^2.83.0",
    "solc": "^0.4.19",
    "web3": "^1.0.0-beta.24"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "nodemon": "^1.8.1"
  }
}
